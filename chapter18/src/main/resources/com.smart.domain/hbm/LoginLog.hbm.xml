<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.smart.domain.LoginLog" table="t_login_log" >
		<id name="loginLogId" column="login_log_id">
			<generator class="identity" />
		</id>
		<property name="ip" column="ip"/>
		<property name="loginDate" column="login_datetime" type="date"/>
		<many-to-one name="user" class="com.smart.domain.User" column="user_id"/>
	</class>
</hibernate-mapping>
